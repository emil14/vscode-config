{
	"Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	},
	"Disable ESLint": {
		"prefix": "eslint-disable",
		"body": ["/* eslint-disable ${1:rule} */", "$0/* eslint-enable ${1} */"],
    "description": "ESLint disable wrapper"
	},
}
